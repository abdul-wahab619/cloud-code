{
  "cli": {
    "version": ">= 9.2.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://dev-api.cloudcode.dev"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "autoIncrement": true,
        "enterpriseProvisioning": "universal"
      },
      "android": {
        "autoIncrement": true,
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://staging-api.cloudcode.dev"
      }
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "ios": {
        "autoIncrement": true,
        "enterpriseProvisioning": "universal"
      },
      "android": {
        "autoIncrement": true,
        "buildType": "app-bundle"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api.cloudcode.dev"
      }
    },
    "production-simulator": {
      "ios": {
        "simulator": true
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api.cloudcode.dev"
      }
    },
    "base-production": {
      "extends": "production"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "your-apple-id@email.com",
        "ascAppId": "YOUR_APP_STORE_CONNECT_APP_ID",
        "appleTeamId": "YOUR_APPLE_TEAM_ID"
      },
      "android": {
        "serviceAccountKeyPath": "./google-service-account.json",
        "track": "internal"
      }
    }
  }
}
