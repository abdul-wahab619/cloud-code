{
  "name": "cloud-code",
  "description": "Build a Container-enabled Worker",
  "version": "0.0.3",
  "private": true,
  "scripts": {
    "build:css": "tailwindcss -c ./tailwind.dashboard.config.js -i ./dashboard/input.css -o ./dashboard/output.css --minify",
    "deploy": "npm run build:css && wrangler deploy",
    "deploy:staging": "npm run build:css && wrangler deploy --env staging",
    "dev": "wrangler dev",
    "start": "wrangler dev",
    "cf-typegen": "wrangler types",
    "test": "vitest run tests/",
    "test:watch": "vitest watch tests/",
    "test:coverage": "vitest run --coverage tests/",
    "test:integration": "vitest run tests/integration.test.ts",
    "test:e2e": "vitest run --config vitest.config.e2e.ts",
    "test:e2e:watch": "vitest watch --config vitest.config.e2e.ts"
  },
  "devDependencies": {
    "@types/node": "^24.0.3",
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.5.2",
    "vitest": "^3.0.0",
    "wrangler": "^4.54.0"
  },
  "dependencies": {
    "@cloudflare/containers": "0.0.8",
    "@tsndr/cloudflare-worker-jwt": "^3.2.0",
    "playwright": "^1.57.0"
  },
  "cloudflare": {
    "label": "Containers Starter",
    "products": [
      "Workers",
      "Durable Objects",
      "Containers"
    ],
    "categories": [],
    "icon_urls": [
      "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/5ca0ca32-e897-4699-d4c1-6b680512f000/public"
    ],
    "preview_image_url": "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/fc7b4b62-442b-4769-641b-ad4422d74300/public"
  }
}
